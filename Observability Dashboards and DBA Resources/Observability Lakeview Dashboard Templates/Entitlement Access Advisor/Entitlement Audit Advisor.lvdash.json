{"datasets":[{"name":"dfb3a6bb","displayName":"Table Level Access","query":"\n-- Table Entities\n\nWITH parsed_tags AS (\n        SELECT *\n        FROM main.access_entitlements_advisor.uc_tags t \n        WHERE (:tag_name_csv = 'all')\n        OR (lower(t.tag_name) IN (SELECT value \n                                FROM LATERAL VIEW explode(split(lower(:tag_name_csv), ',')) AS value))\n),\n\ntable_privs AS (\nSELECT \n* ,\nCASE WHEN p.grantee = ug.user_name THEN 'Direct User Access'\n        WHEN p.grantee = ug.group_name THEN 'Group Access'\n        ELSE 'Unknown Access Source'\n        END AS AccessType\nFROM main.access_entitlements_advisor.all_privileges_snapshot p\nINNER JOIN main.access_entitlements_advisor.user_group_crosswalk AS ug ON\n                                (ug.group_name = p.grantee OR ug.user_name = p.grantee)\n),\n\nclean_tags AS (\nSELECT \n* ,\n-- Catalog Level Tags\n        (SELECT COALESCE(collect_set(tag_name), ARRAY()) FROM parsed_tags tt\n        WHERE tt.entity_type = 'catalog'\n        AND tp.entity_name LIKE(tt.entity_name + '%')\n        ) AS catalog_level_tags,\n-- Schema Level Tags\n        (SELECT COALESCE(collect_set(tag_name), ARRAY()) FROM parsed_tags tt\n        WHERE tt.entity_type = 'schema'\n        AND tp.entity_name LIKE(tt.entity_name + '%')\n        ) AS schema_level_tags,\n-- Table Level Tags\n        (SELECT COALESCE(collect_set(tag_name), ARRAY()) FROM parsed_tags tt\n        WHERE tt.entity_type = 'table'\n        AND tp.entity_name = tt.entity_name\n        ) AS table_level_tags,\n-- Column Level Tags\n        (SELECT COALESCE(collect_set(tag_name), ARRAY()) FROM parsed_tags tt\n        WHERE tt.entity_type = 'column'\n        AND tt.entity_name ILIKE (tp.entity_name +'%')\n        ) AS column_level_tags\nFROM table_privs tp\n)\n\nSELECT \n*\nFROM clean_tags\nWHERE :tag_name_csv = 'all'\nOR size(table_level_tags) > 0\nOR size(column_level_tags) > 0\nOR size(schema_level_tags) > 0\nOR size(catalog_level_tags) > 0\n\n\n","parameters":[{"displayName":"tag_name_csv","keyword":"tag_name_csv","dataType":"STRING","defaultSelection":{"values":{"dataType":"STRING","values":[{"value":"all"}]}}}]}],"pages":[{"name":"1e09eacf","displayName":"New Page","layout":[{"widget":{"name":"07a0673d","queries":[{"name":"dashboards/01ef0023f69a1fb2988fe13b4276bc72/datasets/01ef75e21703189281c43b64489bf0af_user_name","query":{"datasetName":"dfb3a6bb","fields":[{"name":"user_name","expression":"`user_name`"},{"name":"user_name_associativity","expression":"COUNT_IF(`associative_filter_predicate_group`)"}],"disaggregated":false}}],"spec":{"version":2,"widgetType":"filter-multi-select","encodings":{"fields":[{"fieldName":"user_name","displayName":"user_name","queryName":"dashboards/01ef0023f69a1fb2988fe13b4276bc72/datasets/01ef75e21703189281c43b64489bf0af_user_name"}]},"frame":{"showTitle":true,"title":"User Name"}}},"position":{"x":0,"y":7,"width":2,"height":2}},{"widget":{"name":"4a377b14","textbox_spec":"# Data Asset Access  - Show Entitlements Distribution from Either Data Asset (catalog, schema, table, location, volume, etc.) or User Entity\n-----"},"position":{"x":0,"y":0,"width":6,"height":2}},{"widget":{"name":"27eaf114","textbox_spec":"#### User Entity Focused --> \"What access entitlements does User or Group X have today for both read / write?\"\n-----\n#### Data Asset Entity Focused --> \"For these Data Assets, what users / groups have read/write access?\"\n-----"},"position":{"x":0,"y":2,"width":6,"height":2}},{"widget":{"name":"3fdcf706","queries":[{"name":"main_query","query":{"datasetName":"dfb3a6bb","fields":[{"name":"grantee","expression":"`grantee`"},{"name":"entity_type","expression":"`entity_type`"},{"name":"entity_name","expression":"`entity_name`"},{"name":"privilege_level","expression":"`privilege_level`"},{"name":"privilege_type","expression":"`privilege_type`"},{"name":"catalog","expression":"`catalog`"},{"name":"schema","expression":"`schema`"},{"name":"user_id","expression":"`u